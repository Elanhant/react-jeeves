{
  "name": "react-jeeves",
  "version": "0.0.0",
  "description": "React components, hooks and utilities to help you out",
  "main": "index.js",
  "scripts": {
    "lerna": "lerna",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "ts:check": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "prettify": "prettier --write \"packages/**/{src, stories, __tests__}/**/*.{ts,tsx,js}\"",
    "test": "jest --env=jsdom --verbose=false",
    "test:watch": "yarn test --watch",
    "test:generate-output": "yarn test --json --outputFile=.jest-test-results.json || true",
    "commit-test-output": "yarn test:generate-output && git add .jest-test-results.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elanhant/react-jeeves.git"
  },
  "keywords": [
    "React",
    "hooks",
    "side",
    "sheet",
    "toast"
  ],
  "author": "Aleksei Ustiuzhanin <elanhant@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Elanhant/react-jeeves/issues"
  },
  "homepage": "https://github.com/Elanhant/react-jeeves#readme",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@emotion/babel-preset-css-prop": "^10.0.9",
    "@storybook/addon-a11y": "^5.0.6",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-jest": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-notes": "^5.0.6",
    "@storybook/addon-storysource": "^5.0.6",
    "@storybook/addon-viewport": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "@types/storybook__addon-a11y": "^5.0.0",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-jest": "^3.4.2",
    "@types/storybook__addon-viewport": "^4.1.0",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-loader": "^8.0.5",
    "babel-preset-react-app": "^7.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "lerna": "^3.10.8",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hooks-testing-library": "^0.4.0",
    "react-testing-library": "^6.0.4",
    "typescript": "^3.4.1"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {}
